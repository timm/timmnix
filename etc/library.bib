file = {:Users/timm/svns/doc/pso/09reviewPSO.pdf:pdf},
isbn = {9821828841},
issn = {17938201},
journal = {International Journal of Computer Theory and Engineering},
keywords = {Applications.,Heuristic Optimization,Optimization (PSO),Particles Swarm,Taxonomy},
