@article{Pareek2013,
abstract = {Humans may be exceptional learners but they have biological limitations and more- over, inductive biases similar to machine learning algorithms. This puts limits on human learning ability and on the kinds of learning tasks humans can easily han- dle. In this paper, we consider the prob- lem of “boosting” human learners to extend the learning ability of human learners and achieve improved performance on tasks which individual humans find difficult. We consider classification (category learning) tasks, pro- pose a boosting algorithm for human learn- ers and give theoretical justifications. We conduct experiments using Amazon’s Me- chanical Turk on two synthetic datasets – a crosshair task with a nonlinear decision boundary and a gabor patch task with a lin- ear boundary but which is inaccessible to hu- man learners – and one real world dataset – the Opinion Spam detection task introduced in (Ott et al., 2011). Our results show that boosting human learners produces gains in accuracy and can overcome some fundamen- tal limitations of human learners.},
author = {Pareek, Harsh and Ravikumar, Pradeep},
file = {:Users/timm/svns/doc/13humanBoost.pdf:pdf},
